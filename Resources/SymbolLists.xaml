<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:HMI"
                    xmlns:vw="http://inosoft.com/visiwin7">

    <vw:SymbolStateCollection x:Key="SymbolList1">
        <vw:SymbolState Value="0" Symbol="{DynamicResource MBAsterixIcon}"/>
        <vw:SymbolState Value="1..8" Symbol="{DynamicResource MBErrorIcon}"/>
        <vw:SymbolState Value="9,12,15" Symbol="{DynamicResource MBExclamationIcon}"/>        
        <vw:SymbolState Symbol="{DynamicResource MBStopIcon}"/>
    </vw:SymbolStateCollection>

</ResourceDictionary>