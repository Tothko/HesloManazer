    <local:TouchKeyMapping x:Key="TKM_OemCommaNum" KeyCode="OemComma" Regular="," Shift="" TextTopLeft="" TextBottomLeft=""/>
    <local:TouchKeyMapping x:Key="TKM_OemPeriodNum" KeyCode="OemPeriod" Regular="." Shift="" TextTopLeft="" TextBottomLeft="" />
	<local:TouchKeyMapping x:Key="TKM_OemMinus" KeyCode="OemMinus" Regular="-" Shift="" TextTopLeft="" TextBottomLeft="" />

    <local:TouchKeyMapping x:Key="TKM_Nums" KeyCode="" TextCenter="123" Regular="123" Shift="123" Alt="123" AltGr="123" Num="abc" Mark="123"/>
    <local:TouchKeyMapping x:Key="TKM_Marks" KeyCode="" TextCenter="#+=" Regular="#+=" Shift="#+=" Num="#+=" Mark="#+=" />
	
    
