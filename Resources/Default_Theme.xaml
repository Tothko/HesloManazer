<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    xmlns:ice="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options" xmlns:vw="http://inosoft.com/visiwin7">

    <Color x:Key="ApplicationBackgroundColor">#FFFFFF</Color>

    <Color x:Key="PrimaryColor">#2384cb</Color>
    <Color x:Key="PrimaryLightColor">#79addf</Color>
    <Color x:Key="PrimaryExtraLightColor">#9bbfe6</Color>
    <Color x:Key="PrimaryDarkColor">#07497c</Color>
    
    <Color x:Key="ForegroundColor">#FFFFFF</Color>
    <Color x:Key="AlternateForegroundColor">#202020</Color>

    <Color x:Key="ErrorColor">#d32f2f</Color>
    <Color x:Key="BlinkColor">#d32f2f</Color>

    <Color x:Key="ConnectionErrorBorderColor">#B0FFC560</Color>
    <Color x:Key="ConnectionErrorBackgroundColor">#B0FFE97C</Color>

    <Color x:Key="MouseOverLightColor">#EBEBEB</Color>
    <Color x:Key="MouseOverDarkColor">#D6D6D6</Color>

    <Color x:Key="UnderlineMouseOverColor">#202020</Color>

    <Color x:Key="BlueColor">#ECEFF0</Color>
    <Color x:Key="Blue2Color">#DEE2E2</Color>
    <Color x:Key="Blue3Color">#D0D6D8</Color>
    <Color x:Key="Blue4Color">#c2d8f1</Color>
    <Color x:Key="Blue5Color">#246bad</Color>
  
    <Color x:Key="GrayColor">#FAFAFA</Color>
    <Color x:Key="Gray2Color">#F2F2F2</Color>
    <Color x:Key="Gray3Color">#EEEEEE</Color>
    <Color x:Key="Gray4Color">#F5F5F5</Color>    
    <Color x:Key="Gray5Color">#E4E4E4</Color>
    <Color x:Key="Gray6Color">#9E9E9E</Color>
    <Color x:Key="Gray7Color">#666666</Color>
    
    <FontFamily x:Key="FontFamily">Segoe UI</FontFamily>
    <FontWeight x:Key="FontWeight">Normal</FontWeight>

    <system:Double x:Key="FontSize_AlphaTouchpad">42</system:Double>
    <system:Double x:Key="FontSize_NumericTouchpad">36</system:Double>
    <system:Double x:Key="FontSize_TouchpadHeadline">20</system:Double>

    <system:Double x:Key="FontSize_Headline">32</system:Double>
    <system:Double x:Key="FontSize_Title">24</system:Double>
    <system:Double x:Key="FontSize_HeaderComponents">20</system:Double>
    <system:Double x:Key="FontSize_Label">18</system:Double>
    <system:Double x:Key="FontSize_Subheader">16</system:Double>
    <system:Double x:Key="FontSize_Normal">16</system:Double>
    <system:Double x:Key="FontSize_Caption">14</system:Double>

    <system:Double x:Key="FontSize_GaugeValue">48</system:Double>

    <CornerRadius x:Key="CornerRadius">2</CornerRadius>

    <system:Double x:Key="ScrollBarThickness">8</system:Double>

    <SolidColorBrush x:Key="ApplicationBackgroundBrush" Color="{StaticResource ApplicationBackgroundColor}" ice:Freeze="True"/>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}" ice:Freeze="True"/>
    <SolidColorBrush x:Key="PrimaryLightBrush" Color="{StaticResource PrimaryLightColor}" ice:Freeze="True"/>
    <SolidColorBrush x:Key="PrimaryExtraLightBrush" Color="{StaticResource PrimaryExtraLightColor}" ice:Freeze="True"/>
    <SolidColorBrush x:Key="PrimaryDarkBrush" Color="{StaticResource PrimaryDarkColor}" ice:Freeze="True"/>

    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource ForegroundColor}" ice:Freeze="True"/>
    <SolidColorBrush x:Key="ForegroundDarkBrush" Color="{StaticResource AlternateForegroundColor}" ice:Freeze="True"/>

    <SolidColorBrush x:Key="BlinkBrush" Color="{StaticResource BlinkColor}" ice:Freeze="True"/>

    <SolidColorBrush x:Key="MouseOverLightBrush" Color="{StaticResource MouseOverLightColor}" ice:Freeze="True"/>
    <SolidColorBrush x:Key="MouseOverDarkBrush" Color="{StaticResource MouseOverDarkColor}" ice:Freeze="True"/>

    <SolidColorBrush x:Key="UnderlineMouseOverBrush" Color="{StaticResource UnderlineMouseOverColor}" ice:Freeze="True"/>

    <SolidColorBrush x:Key="ConnectionErrorBorderBrush" Color="{StaticResource ConnectionErrorBorderColor}" ice:Freeze="true" />
    <SolidColorBrush x:Key="ConnectionErrorBackgroundBrush" Color="{StaticResource ConnectionErrorBackgroundColor}" ice:Freeze="true" />

    <SolidColorBrush x:Key="ErrorBrush" Color="{StaticResource ErrorColor}" ice:Freeze="true" />

    <SolidColorBrush x:Key="GrayBrush" Color="{StaticResource GrayColor}" ice:Freeze="true" />
    <SolidColorBrush x:Key="Gray2Brush" Color="{StaticResource Gray2Color}" ice:Freeze="true" />
    <SolidColorBrush x:Key="Gray3Brush" Color="{StaticResource Gray3Color}" ice:Freeze="true" />
    <SolidColorBrush x:Key="Gray4Brush" Color="{StaticResource Gray4Color}" ice:Freeze="true" />
    <SolidColorBrush x:Key="Gray5Brush" Color="{StaticResource Gray5Color}" ice:Freeze="true" />
    <SolidColorBrush x:Key="Gray6Brush" Color="{StaticResource Gray6Color}" ice:Freeze="True"/>
    <SolidColorBrush x:Key="Gray7Brush" Color="{StaticResource Gray7Color}" ice:Freeze="True"/>

    <SolidColorBrush x:Key="BarBackgroundBrush" Color="Black" Opacity="0.05" ice:Freeze="true" />
    
    <SolidColorBrush x:Key="BlueBrush" Color="{StaticResource BlueColor}" ice:Freeze="true" />
    <SolidColorBrush x:Key="Blue2Brush" Color="{StaticResource Blue2Color}" ice:Freeze="true" />
    <SolidColorBrush x:Key="Blue3Brush" Color="{StaticResource Blue3Color}" ice:Freeze="true" />
    <SolidColorBrush x:Key="Blue4Brush" Color="{StaticResource Blue4Color}" ice:Freeze="true" />
    <SolidColorBrush x:Key="Blue5Brush" Color="{StaticResource Blue5Color}" ice:Freeze="true" />
  
    <SolidColorBrush x:Key="NavigationContainerPanelBackgroundBrush" Color="{StaticResource Gray3Color}" ice:Freeze="true" />
    <SolidColorBrush x:Key="AlternateNavigationContainerPanelBackgroundBrush" Color="Transparent" ice:Freeze="true" />
    
</ResourceDictionary>